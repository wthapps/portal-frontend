{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "portal": {
      "root": "src",
      "sourceRoot": "src/portal",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/portal",
            "index": "src/portal/index.html",
            "main": "src/portal/main.ts",
            "polyfills": "src/portal/polyfills.ts",
            "tsConfig": "src/portal/tsconfig.app.json",
            "assets": [
              { "glob": "**/*", "input": "src/assets/", "output": "/assets/" }
            ],
            "styles": [
              "node_modules/primeng/resources/themes/omega/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/cropperjs/dist/cropper.min.css",
              "src/portal/styles/portal.scss",
              "src/assets/styles/styles-app-loading.scss"
            ],
            "scripts": [
              "node_modules/cropperjs/dist/cropper.min.js",
              "src/shared/channels/common.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "serviceWorker": false,
              "fileReplacements": [
                {
                  "replace":
                  "src/portal/../environments/environment.ts",
                  "with":
                  "src/portal/../environments/environment.prod.ts"
                }
              ]
            },
            "stag": {
              "fileReplacements": [
                {
                  "replace": "src/portal/../environments/environment.ts",
                  "with": "src/portal/../environments/environment.stag.ts"
                }
              ]
            },
            "test": {
              "fileReplacements": [
                {
                  "replace": "src/portal/../environments/environment.ts",
                  "with": "src/portal/../environments/environment.test.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "portal:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "portal:build:production"
            },
            "stag": {
              "browserTarget": "portal:build:stag"
            },
            "test": {
              "browserTarget": "portal:build:test"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "portal:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/portal/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/portal/polyfills.ts",
            "tsConfig": "src/portal/tsconfig.spec.json",
            "scripts": [
              "node_modules/cropperjs/dist/cropper.min.js",
              "src/shared/channels/common.js"
            ],
            "styles": [
              "src/assets/styles/styles-app-loading.scss",
              "node_modules/primeng/resources/themes/omega/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/cropperjs/dist/cropper.min.css",
              "src/portal/styles/portal.scss"
            ],
            "assets": [

            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
