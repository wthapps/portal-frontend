{
  "name": "Portal-frontend",
  "version": "1.2.16",
  "description": "All in one web modules",
  "repository": {
    "url": "www.wthapps.com"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build.prod": "ng build --e prod app-name",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start-sample": "ng serve sample --port 2500 --open",
    "start-portal": "ng serve portal --port 3000 --open",
    "start-portal-nr": "ng serve portal --port 3000 --live-reload false --open",
    "start-account": "ng serve my-account --port 3005 --open",
    "start-account-nr": "ng serve my-account --port 3005 --live-reload false --open",
    "start-photo": "ng serve media --port 3010 --open",
    "start-photo-nr": "ng serve media --port 3010 --live-reload false --open",
    "start-social": "ng serve social --port 3015 --open",
    "start-social-nr": "ng serve social --port 3015 --live-reload false --open",
    "start-chat": "ng serve chat --port 3020 --open",
    "start-chat-nr": "ng serve chat --port 3020 --live-reload false --open",
    "start-contact": "ng serve contact --port 3025 --open",
    "start-contact-nr": "ng serve contact --port 3025 --live-reload false --open",
    "start-note": "ng serve note --port 3030 --open",
    "start-note-nr": "ng serve note --port 3030 --live-reload false --open",
    "start-drive": "ng serve drive --port 3035 --open",
    "start-drive-nr": "ng serve drive --port 3035 --live-reload false --open",
    "prettier": "prettier {src,e2e}/**/**/*.{ts,scss} --write",
    "anl-social": "webpack-bundle-analyzer dist/social/stats.json",
    "stats-social": "ng build social --stats-json && webpack-bundle-analyzer dist/social/stats.json",
    "stats-portal": "ng build portal --stats-json && webpack-bundle-analyzer dist/portal/stats.json",
    "stats-contact": "ng build contact --stats-json && webpack-bundle-analyzer dist/contact/stats.json",
    "stats-note": "ng build note --stats-json && webpack-bundle-analyzer dist/note/stats.json",
    "stats-chat": "ng build chat --stats-json && webpack-bundle-analyzer dist/chat/stats.json",
    "stats-drive": "ng build drive --stats-json && webpack-bundle-analyzer dist/drive/stats.json",
    "build-portal": "ng build --prod portal && http-server -p 3000 dist/portal",
    "build-deploy-portal": "ng build --prod portal && cp -R dist/portal/* ~/deployment/fe-portal",
    "build-account": "ng build --prod my-account && http-server -p 3005 dist/my-account",
    "build-contact": "ng build --prod contact && http-server -p 3025 dist/contact",
    "build-note": "ng build --prod note && http-server -p 3030 dist/note",
    "cc-contact-sw": "concat dist/contact/sw.js dist/contact/ngsw-worker.js -o dist/contact/combined-worker.js",
    "build-test-contact": "npm run build-contact && http-server -p 3025 dist/contact",
    "build-social": "ng build --prod social && http-server -p 3015 dist/social",
    "build-deploy-contact": "ng build --prod contact && cp -R dist/contact/* ~/deployment/fe-contact",
    "build-contact-stag": "ng build --prod --e stag contact && http-server -p 3025 dist/contact",
    "build-contact-test": "ng build --prod --configuration=test contact && http-server -p 3025 dist/contact",
    "build-social:staging": "ng build --prod social --env stag",
    "build-portal:staging": "ng build --prod portal --env stag",
    "build-contact:staging": "ng build --prod contact --env stag",
    "ng build all": "build --prod --build-optimizer --aot portal && build --prod --build-optimizer --aot my-account && build --prod --build-optimizer --aot media && build --prod --build-optimizer --aot social && build --prod --build-optimizer --aot contact && build --prod --build-optimizer --aot note"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "0.10.4",
    "@angular/cli": "7.0.4",
    "@angular/compiler-cli": "7.0.2",
    "@angular/language-service": "7.0.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^8.10.45",
    "codelyzer": "~4.3.0",
    "concat": "^1.0.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "1.11.1",
    "protractor": "~5.3.0",
    "source-map-explorer": "^1.6.0",
    "ts-node": "~5.0.1",
    "tslint": "^5.11.0",
    "typescript": "3.1.6",
    "webpack-bundle-analyzer": "3.6.0"
  },
