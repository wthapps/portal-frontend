    "svg-country-flags": "1.1.2",
    "systemjs": "0.20.19",
    "tether-shepherd": "1.8.1",
    "uppy": "0.27.3",
    "uuid": "^3.3.2",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  }
}
