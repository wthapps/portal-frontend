    "@uppy/aws-s3-multipart": "^0.30.3",
    "@uppy/companion": "^0.17.3",
    "actioncable": "5.1.2",
    "angular-2-local-storage": "^3.0.2",
    "angular2-hotkeys": "2.0.3",
    "angular2-recaptcha": "1.1.0",
    "aws-s3": "^2.0.5",
    "boom": "^7.2.2",
    "bootstrap": "3.3.7",
    "bootstrap-sass": "3.3.7",
    "classlist.js": "^1.1.20150312",
    "core-js": "2.5.4",
    "cropperjs": "1.1.3",
    "dragselect": "1.7.23",
    "dropzone": "5.1.1",
    "emojione-assets": "^3.1.1",
