{
  "name": "Portal-frontend",
  "version": "2.0.0",
  "description": "All in one web modules",
  "repository": {
    "url": "www.wthapps.com"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build.prod": "ng build --e prod --app app-name",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start-portal": "ng serve --app portal --port 3000 --live-reload true --open",
    "start-my": "ng serve --app my-account --port 3005 --live-reload true --open",
    "start-media": "ng serve --app media --port 3010 --live-reload true --open",
    "start-media-false": "ng serve --app media --port 3010 --live-reload false --open",
    "start-social": "ng serve --app social --port 3015 --live-reload true --open",
    "start-social-false": "ng serve --app social --port 3015 --live-reload false --open",
    "start-chat": "ng serve --app chat --port 3020 --live-reload true --open",
    "start-chat-false": "ng serve --app chat --port 3020 --live-reload false --open",
    "start-contact": "ng serve --app contact --port 3025 --live-reload true --open",
    "start-contact-false": "ng serve --app contact --port 3025 --live-reload false --open",
    "start-note": "ng serve --app note --port 3030 --live-reload true --open",
    "prettier": "prettier {src,e2e}/**/**/*.{ts,scss} --write",
    "anl-social": "webpack-bundle-analyzer dist/social/stats.json",
    "stats-social": "ng build --app social --stats-json && webpack-bundle-analyzer dist/social/stats.json",
    "stats-note": "ng build --app note --stats-json && webpack-bundle-analyzer dist/note/stats.json",
    "stats-contact": "ng build --app contact --stats-json && webpack-bundle-analyzer dist/contact/stats.json",
    "build-deploy-test": "ENV=test ./deploy/build_deploy_all_local.sh | tee build_deploy_test.log",
    "build-portal": "ng build --prod --app portal && http-server -p 3000 dist/portal",
    "build-deploy-portal": "ng build --prod --app portal && cp -R dist/portal/* ~/deployment/fe-portal",
    "build-my-account": "ng build --prod --app my-account && http-server -p 3005 dist/my-account",
    "build-note": "ng build --prod --app note && http-server -p 3030 dist/note",
    "cc-contact-sw": "concat dist/contact/sw.js dist/contact/ngsw-worker.js -o dist/contact/combined-worker.js",
    "build-contact": "ng build --prod --app contact && npm run cc-contact-sw",
    "build-test-contact": "npm run build-contact && http-server -p 3025 dist/contact",
    "build-social": "ng build --prod --app social && http-server -p 3015 dist/social",
    "build-deploy-contact": "ng build --prod --app contact && cp -R dist/contact/* ~/deployment/fe-contact",
    "build-contact-stag": "ng build --prod --e stag --app contact && http-server -p 3025 dist/contact",
    "build-social:staging": "ng build --prod --app social --env stag",
    "build-portal:staging": "ng build --prod --app portal --env stag",
    "build-contact:staging": "ng build --prod --app contact --env stag",
    "ng build all": "build --prod --build-optimizer --aot --app portal && build --prod --build-optimizer --aot --app my-account && build --prod --build-optimizer --aot --app media && build --prod --build-optimizer --aot --app social && build --prod --build-optimizer --aot --app contact && build --prod --build-optimizer --aot --app note"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "0.6.8",
    "@angular/cli": "6.0.8",
    "@angular/compiler-cli": "6.0.6",
    "@angular/language-service": "6.0.6",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "concat": "^1.0.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "1.11.1",
    "protractor": "~5.3.0",
    "source-map-explorer": "^1.5.0",
    "ts-node": "~5.0.1",
    "tslint": "^5.10.0",
    "typescript": "2.7.2",
    "webpack-bundle-analyzer": "^2.11.1"
  },
  "dependencies": {
    "@angular/animations": "6.0.6",
    "@angular/cdk": "6.3.1",
    "@angular/common": "6.0.6",
    "@angular/compiler": "6.0.6",
    "@angular/core": "6.0.6",
    "@angular/forms": "6.0.6",
    "@angular/http": "6.0.6",
    "@angular/material": "6.3.1",
    "@angular/platform-browser": "6.0.6",
    "@angular/platform-browser-dynamic": "6.0.6",
    "@angular/pwa": "0.6.8",
    "@angular/router": "6.0.6",
    "@angular/service-worker": "6.0.6",
    "@ngrx/effects": "6.0.1",
    "@ngrx/entity": "6.0.1",
    "@ngrx/router-store": "6.0.1",
    "@ngrx/store": "6.0.1",
    "@ngrx/store-devtools": "6.0.1",
    "@nicky-lenaers/ngx-scroll-to": "^0.6.1",
    "@types/jquery": "^3.3.2",
    "@types/lodash": "^4.14.109",
    "actioncable": "5.1.2",
    "adyen-cse-web": "git+https://github.com/Adyen/adyen-cse-web.git#v0.1.22",
    "angular2-hotkeys": "2.0.3",
    "angular2-recaptcha": "1.1.0",
    "boom": "^7.2.0",
    "bootstrap": "3.3.7",
    "bootstrap-sass": "3.3.7",
    "core-js": "2.5.4",
    "cropperjs": "1.1.3",
    "dragselect": "1.7.23",
    "dropzone": "5.1.1",
    "emojione-assets": "^3.1.1",
    "file-saver": "1.3.3",
    "font-awesome": "4.7.0",
    "http-status-codes": "^1.3.0",
    "intl": "1.2.5",
    "katex": "^0.8.3",
    "lazysizes": "^4.0.1",
    "linkifyjs": "2.1.5",
    "lodash": "^4.17.10",
    "moment": "2.19.4",
    "ng2-bs3-modal": "0.13.0",
    "ng2-rx-componentdestroyed": "3.0.0",
    "ngrx-store-freeze": "0.2.4",
    "ngx-chips": "1.9.2",
    "ng2-cable": "2.0.7",
    "ng2-sticky-kit": "^5.3.0",
    "ngx-cookie": "2.0.1",
    "ngx-dropzone-wrapper": "^5.3.4",
    "ngx-infinite-scroll": "6.0.1",
    "payment": "^2.3.0",
    "primeng": "6.0.0",
    "quill": "1.3.5",
    "quill-delta": "3.6.2",
    "quill-image-resize-module": "3.0.0",
    "redux": "^4.0.0",
    "reselect": "^3.0.1",
    "rxjs": "^6.2.1",
    "rxjs-compat": "^6.2.1",
    "rxjs-tslint": "^0.1.5",
    "slick-carousel": "1.6.0",
    "svg-country-flags": "1.1.2",
    "systemjs": "0.20.19",
    "tether-shepherd": "1.8.1",
    "uppy": "^0.26.0",
    "zone.js": "0.8.26"
  }
}
