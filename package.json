    "ngx-cookie": "2.0.1",
    "ngx-drag-to-select": "^3.1.1",
    "ngx-dropzone-wrapper": "^5.3.4",
    "ngx-infinite-scroll": "7.1.0",
    "parchment": "^1.1.4",
    "phoenix": "^1.4.1",
